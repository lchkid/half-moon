/* components/hm-tabswitch/hm-tabswitch.wxss */
.tab-switch {
  display: flex;

  font-size: 28rpx;
  line-height: 28rpx;
  text-align: center;
  margin: 10rpx 0;
}

.ts-item {
  border: 1px solid #eeeeee;
  border-radius: 28rpx;
  padding: 10rpx 20rpx;
  margin-left: 20rpx;
}

.active {
  color: #FF8198;
  border-color: #FF8198;
}